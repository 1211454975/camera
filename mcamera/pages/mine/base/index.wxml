<!--pages/user/user.wxml-->
<view class=''>
  <view class='user'>
    <navigator url="../../../packageC/pages/mine/user-edit/index">
      <image class='userimg' src='{{user.headImg||"https://gitee.com/hawklink/motionsdk_oss/raw/master/images/user.png"}}'></image>
    </navigator>
    <view class='userinfo'>
      <text class='username'>{{user.name||user.nickname}}</text>
      <text class='tel'>{{user.tel}}</text>
      <view>
        <text class='cell'>{{user.comName||"default"}}</text>
      </view>
      
    </view>
    <navigator class='code' url='../../../packageC/pages/mine/user-code/index'>
      <image src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/user/user-code.png'></image>
    </navigator>
  </view>
