  <view class='list' style="margin-top:30px;">
    <view class='section'>
      <navigator class='row' url='../../../packageC/pages/mine/user-profile/index'>
        <view class='left'>
          <image class='icon' src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/user/user-1-1.png'></image>
          <text class='text'>我的资料</text>
        </view>
        <image class='arrow' src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/arrow-right.png'></image>
      </navigator>
      <navigator class='row' url='../audit/index'>
        <view class='left'>
          <image class='icon' src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/user/user-1-3.png'></image>
          <text class='text'>功能测试</text>
        </view>
        <image class='arrow' src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/arrow-right.png'></image>
      </navigator>
      <navigator class='row' url='../poster/index'>
        <view class='left'>
          <image class='icon' src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/user/user-1-3.png'></image>
          <text class='text'>小海报</text>
        </view>
        <image class='arrow' src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/arrow-right.png'></image>
      </navigator>
    </view>
    
    <view class='section'>
      <view class='row'>
        <button open-type="share"></button>
        <view class='left'>
          <image class='icon' src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/user/user-3-1.png'></image>
          <text class='text'>分享给朋友</text>
        </view>
        <image class='arrow' src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/arrow-right.png'></image>
      </view>
    </view>
  </view>

  <view class='list'>
    <view class='section'>
      <navigator class='row' url='../../../packageC/pages/admin/login/index'>
        <view class='left'>
          <image class='icon' src='https://gitee.com/hawklink/motionsdk_oss/raw/master/images/user/user-1-1.png'></image>
